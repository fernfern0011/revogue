{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/account",
            "dest": "/server.js"
          },
        {
            "src": "/api/(.*)",
            "dest": "/server.js"
        }
    ]
}